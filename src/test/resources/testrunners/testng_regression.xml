<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Regression PW Suite" thread-count ="2" parallel="tests" verbose="4">

    <listeners>
        <listener
                class-name="com.qa.ecommerce.listeners.ExtentReportListener" />
    </listeners>


    <test name="Open Cart Home Page Test">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="com.qa.ecommerce.tests.HomePageTest" />
        </classes>
    </test>

    <test name="Open Cart Login Page Test">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="com.qa.ecommerce.tests.LoginPageTest" />
        </classes>
    </test>

</suite>